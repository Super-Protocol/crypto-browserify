{
	"author": "Dominic Tarr <dominic.tarr@gmail.com> (dominictarr.com)",
	"name": "crypto-browserify",
	"description": "implementation of crypto for the browser",
	"version": "3.12.0",
	"homepage": "https://github.com/browserify/crypto-browserify",
	"repository": {
		"type": "git",
		"url": "git://github.com/browserify/crypto-browserify.git"
	},
	"scripts": {
		"lint": "eslint --ext=js,mjs .",
		"pretest": "npm run lint",
		"tests-only": "tape 'test/**/*.js'",
		"test": "npm run tests-only",
		"posttest": "aud --production",
		"browser": "zuul --browser-version $BROWSER_VERSION --browser-name $BROWSER_NAME -- test/index.js"
	},
	"engines": {
		"node": "*"
	},
	"dependencies": {
		"browserify-cipher": "^1.0.0",
		"browserify-sign": "^4.0.0",
		"create-ecdh": "^4.0.0",
		"create-hash": "^1.1.0",
		"create-hmac": "^1.1.0",
		"diffie-hellman": "^5.0.0",
		"inherits": "^2.0.1",
		"pbkdf2": "^3.0.3",
		"public-encrypt": "^4.0.0",
		"randombytes": "^2.0.0",
		"randomfill": "^1.0.3"
	},
	"devDependencies": {
		"@ljharb/eslint-config": "^21.1.0",
		"aud": "^2.0.3",
		"eslint": "=8.8.0",
		"hash-test-vectors": "^1.3.2",
		"pseudorandombytes": "^2.0.0",
		"safe-buffer": "^5.2.1",
		"tape": "^5.7.2",
		"zuul": "^3.12.0"
	},
	"browser": {
		"crypto": false
	},
	"license": "MIT"
}
